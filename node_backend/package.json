{
  "name": "node_backend",
  "main": "index.js",
  "scripts": {
    "test": "vitest",
    "dev": "tsx watch --inspect=0.0.0.0:9229 src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "db:1migrate_create": "npx prisma migrate dev --create-only",
    "db:2migrate_apply": "npx prisma migrate dev --skip-generate",
    "db:3typegen": "npx kysely-codegen --dialect postgres --out-file ./src/db/db.d.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "description": "",
  "packageManager": "pnpm@9.15.4",
  "dependencies": {
    "@prisma/client": "^6.3.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "kysely": "^0.27.5",
    "pg": "^8.13.3",
    "prisma": "^6.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.4",
    "@types/pg": "^8.11.11",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.15.0",
    "kysely-codegen": "^0.17.0",
    "prettier": "^3.5.1",
    "supertest": "^7.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "vitest": "^3.0.5"
  },
  "kysely": {
    "output": "./src/types/kysely.d.ts"
  }
